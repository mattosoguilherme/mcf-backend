// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(uuid())
  login String
  senha String

  @@map("user")
}

model Marmita {
  id             String @id @default(uuid())
  nome           String
  quantidade     Int
  tipo_encomenda String

  @@map("marmita")
}
